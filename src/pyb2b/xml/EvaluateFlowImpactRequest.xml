<flig:EvaluateFlowImpactRequest xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:flig="eurocontrol/cfmu/b2b/FlightServices"
    xmlns:ns="http://www.fixm.aero/flight/4.2"
    xmlns:ns1="http://www.fixm.aero/base/4.2">
    <sendTime>{send_time:%Y-%m-%d %H:%M:%S}</sendTime>
    <flightPlan>
        <structured>
            <flightPlan>
                <aerodromeOfDeparture>
                    <icaoId>{aerodromeOfDeparture}</icaoId>
                </aerodromeOfDeparture>
                <aerodromesOfDestination>
                    <aerodromeOfDestination>
                    <icaoId>{aerodromeOfDestination}</icaoId>
                    </aerodromeOfDestination>
                </aerodromesOfDestination>
                <aircraftId>
                    <aircraftId>{aircraftId}</aircraftId>
                    <registrationMark>{registrationMark}</registrationMark>
                </aircraftId>
                <numberOfAircraft>{numberOfAircraft}</numberOfAircraft>
                <aircraftType>
                    <icaoId>{aircraftType}</icaoId>
                </aircraftType>
                <totalEstimatedElapsedTime>{totalEstimatedElapsedTime}</totalEstimatedElapsedTime>
                <wakeTurbulenceCategory>{wakeTurbulenceCategory}</wakeTurbulenceCategory>
                <flightType>{flightType}</flightType>
                <flightRules>{flightRules}</flightRules>
                <estimatedOffBlockTime>{estimatedOffBlockTime}</estimatedOffBlockTime>
                <icaoRoute>{icaoRoute}</icaoRoute>
                <equipmentCapabilityAndStatus>
                    {equipmentCapabilityAndStatus}
                </equipmentCapabilityAndStatus>
                <surveillanceEquipment>
                    {surveillanceEquipment}
                </surveillanceEquipment>
                <otherInformation>
                    {otherInformation}
                </otherInformation>
            </flightPlan>
        </structured>
    </flightPlan>
</flig:EvaluateFlowImpactRequest>